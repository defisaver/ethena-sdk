  "dependencies": {
    "@defisaver/positions-sdk": "^2.1.60",
    "@defisaver/sdk": "^1.3.18",
    "@defisaver/tokens": "^1.7.22",
    "bn.js": "^5.1.3",
    "decimal.js": "^10.6.0",
    "viem": "^2.37.9"
  },
  "devDependencies": {
    "@defisaver/eslint-config": "^1.0.1",
    "@metamask/eth-json-rpc-middleware": "^15.0.1",
